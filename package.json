{
  "displayName": "TypeDoc",
  "name": "@disqada/typedoc",
  "version": "1.3.2",
  "description": "Generate types and docs with a click",
  "scripts": {
    "postinstall": "node ./scripts/config.js",
    "config": "node ./bin/config.js",
    "clean": "node ./bin/clean.js",
    "types": "tsc -p ./config/tsconfig.json && npm run clean",
    "doc": "typedoc --options ./config/typedoc.json",
    "docs": "typedoc --options ./config/typedoc.json",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "format": "prettier . --write --ignore-path .gitignore",
    "test": "npx mocha ./test/**/*.test.js"
  },
  "bin": {
    "config": "./bin/config.js",
    "clean": "./bin/clean.js"
  },
  "type": "module",
  "engines": {
    "node": ">=13.2.0"
  },
  "files": [
    "src",
    "bin",
    "template",
    "CHANGELOG.md",
    "README.md",
    "LICENSE.txt"
  ],
  "homepage": "https://disqada.github.io/TypeDoc",
  "repository": {
    "type": "git",
    "url": "https://github.com/DisQada/TypeDoc.git"
  },
  "bugs": {
    "url": "https://github.com/DisQada/TypeDoc/issues"
  },
  "keywords": [
    "types",
    "typings",
    "jsdoc",
    "tsdoc",
    "typedoc",
    "docs",
    "documentation",
    "generated"
  ],
  "author": {
    "name": "The Alpha",
    "email": "contact@nabilalsaiad.com",
    "url": "https://links.nabilalsaiad.com"
  },
  "contributors": [],
  "license": "Apache-2.0",
  "peerDependencies": {
    "@mxssfd/typedoc-theme": "1.1.x",
    "typedoc": "0.25.x",
    "typedoc-plugin-extras": "3.0.x",
    "typedoc-plugin-missing-exports": "2.1.x",
    "typescript": "5.2.x"
  },
  "devDependencies": {
    "eslint": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jsdoc": "*",
    "eslint-plugin-mocha": "*",
    "eslint-plugin-promise": "*",
    "mocha": "^10.2.0",
    "prettier": "*"
  }
}
